{
	"name": "towardstudio/password-protection",
	"description": "Create password protected pages.",
	"type": "craft-plugin",
	"version": "1.0.0",
	"license": "MIT",
	"authors": [
		{
			"name": "Toward Studio",
			"homepage": "https://toward.studio"
		}
	],
	"require": {
		"craftcms/cms": "^5.0.0"
	},
	"autoload": {
		"psr-4": {
			"towardstudio\\passwordprotection\\": "src/"
		}
	},
	"extra": {
		"name": "Password Protection",
		"handle": "passwordprotection",
		"developer": "Toward Studio",
		"developerUrl": "https://toward.studio",
		"documentationUrl": "https://github.com/towardstudio/password-protection/blob/main/README.md",
		"changelogUrl": "https://raw.githubusercontent.com/towardstudio/password-protection/main/CHANGELOG.md",
		"class": "towardstudio\\passwordprotection\\PasswordProtection"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
